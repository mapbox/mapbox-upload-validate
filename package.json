    "split": "~1.0.0",
    "step": "~0.0.6",
    "@mapbox/tilejson": "^1.0.3",
    "@mapbox/tilelive": "~5.12.0",
    "@mapbox/tiletype": "~0.3.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "@mapbox/duplicate-module-test": "^0.1.0",
    "coveralls": "~2.11.2",
    "eslint": "~1.00.0",
    "eslint-config-unstyled": "^1.1.0",
